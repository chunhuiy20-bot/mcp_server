# 开发环境配置 - 可以提交到 Git
# 使用非敏感的开发环境凭证

# MySQL 配置（开发环境）
MYSQL_CONFIG_ASYNC=mysql+aiomysql://dev_user:dev_pass@localhost:3306/dev_db
MYSQL_CONFIG_ASYNC2=mysql+aiomysql://dev_user:dev_pass@localhost:3306/dev_db2
MYSQL_CONFIG_ASYNC_FINANCE_DB=mysql+aiomysql://dev_user:dev_pass@localhost:3306/dev_finance

# Redis 配置（开发环境）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=dev_password
REDIS_DB=0

# OpenAI 配置（开发环境 - 使用测试 key）
OPENAI_API_KEY=sk-test-key-for-development
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY2=sk-test-key-2
OPENAI_BASE_URL2=https://api.openai.com/v1

# 阿里云 OSS（开发环境）
ALIYUN_AK_ID=dev-access-key-id
ALIYUN_AK_SECRET=dev-access-key-secret
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com

# 数据库连接池配置
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=1800

# MongoDB 配置
MONGODB_URI=mongodb://localhost:27017
MONGODB_DB=dev_database
MONGODB_CHECKPOINT_COLLECTION_NAME=checkpoints
MONGODB_WRITES_COLLECTION_NAME=writes

# Nacos 配置
NACOS_SERVER=localhost:8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos
NACOS_NAMESPACE=dev

# 服务配置
SERVICE_NAME=hdd-agent-module
SERVICE_GROUP=DEFAULT_GROUP
SERVICE_PORT=8000
SERVICE_IP=auto
EPHEMERAL=true

# 向量数据库配置
VECTOR_DB_URL=http://localhost:6333
VECTOR_DB_TOKEN=dev-token
